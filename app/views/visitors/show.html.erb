
<h1>Website views</h1>

 <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Country</th>
      <th scope="col">Region</th>
      <th scope="col">City</th>
      <th scope="col">Time</th>
    </tr>
  </thead>
  <tbody>
    <% @visitors.each do |visitor| %>
      <tr>
        <th><%= visitor.id %></td>
        <th><%= visitor.country %></td>
        <th><%= visitor.region %></td>
        <th><%= visitor.city %></td>
        <th><%= visitor.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>